<VBox xmlns:fx="http://javafx.com/fxml/1" spacing = "10" alignment="center" fx:controller="controller.AddToCatalogueController" prefWidth="300" >
    
 

     <GridPane alignment="CENTER" hgap="10" vgap="10">
        <Label  text="Type:" GridPane.columnIndex="0" GridPane.rowIndex="0"/>   
        <TextField fx:id="typeTf" text="" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
        <Label text="Name:" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
       <TextField fx:id="nameTf" text="" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
       <Label text="Price:" GridPane.columnIndex="0" GridPane.rowIndex="2"/>  
       <TextField fx:id="priceTf" text="" GridPane.columnIndex="1" GridPane.rowIndex="2"/>        
    </GridPane> 

<HBox spacing="10" alignment="CENTER">    
     <Button styleClass = "button1" text = "Add to Catalogue" fx:id = "addPart" onAction="#handleAddPart" />
</HBox> 

  
	<stylesheets>
		<URL value="@computerbuilder.css" />
	</stylesheets>
</VBox>

